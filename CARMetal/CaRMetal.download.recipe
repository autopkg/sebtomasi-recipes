<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads latest CaRMetal disk image with JRE included.</string>
    <key>Identifier</key>
    <string>com.github.sebtomasi.download.CaRMetal</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>CaRMetal</string>
        <key>SEARCH_URL</key>
        <string>https://carmetal.org/index.php/fr/telecharger</string>
        <key>SEARCH_PATTERN</key>
        <string>(?P&lt;url&gt;installeurs-CaRMetal/v[0-9]-[0-9]-[0-9]/avecJRE/CaRMetal.dmg)</string>
        <key>DL_URL</key>
        <string>https://carmetal.org</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.9</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>URLTextSearcher</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%SEARCH_URL%</string>
                <key>re_pattern</key>
                <string>%SEARCH_PATTERN%</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>%DL_URL%/%match%</string>
                <key>filename</key>
                <string>%NAME%.dmg</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
</dict>
</plist>
